{
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnSave": true,
    },
    "python.formatting.provider": "autopep8",
    "autopep8.args": [
        "--max-line-length",
        "160"
    ],
    "flake8.args": [
        "--max-line-length=160",
        "--ignore=E501"
    ],
    "python.analysis.extraPaths": [
    ],
    "cSpell.enableFiletypes": [
        "bat",
        "markdown",
        "powershell",
        "shellscript"
    ],
    "cSpell.enabled": true,
    "cSpell.words": [
        "ALLUSERSPROFILE",
        "chcp",
        "choco",
        "Classpath",
        "COMPUTERNAME",
        "conda",
        "conemu",
        "dmzt",
        "dztm",
        "ERRORLEVEL",
        "findstr",
        "gitmodules",
        "HOMEDRIVE",
        "icontains",
        "imatch",
        "impbus",
        "inet",
        "inotcontains",
        "inotmatch",
        "ireplace",
        "keyvault",
        "KMGTPEZY",
        "LASTEXITCODE",
        "localbox",
        "muid",
        "Newtonsoft",
        "notmatch",
        "nproc",
        "objd",
        "opengit",
        "popd",
        "proto",
        "protoc",
        "psall",
        "pskill",
        "pushd",
        "pwsh",
        "pycache",
        "qualiu",
        "realpath",
        "robocopy",
        "setx",
        "Singleline",
        "spotfront",
        "tolower",
        "toplevel",
        "USERDNSDOMAIN",
        "USERPROFILE",
        "viasf",
        "wwwroot",
        "XMOC"
    ]
}